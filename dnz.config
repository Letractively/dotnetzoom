}

var AvailableLanguages = 
{
	'en'	: 'English',
	'fr'	: 'Franais'
}

function returnLanguageCodes()
{
if (  ActiveLanguageCode = 'fr'  ) return frLang ;
else 
return enLang;
}

function GetActiveLanguage() 
{
		var sUserLang ;
		
		
		if ( navigator.userLanguage )
			sUserLang = navigator.userLanguage.toLowerCase() ;
		else if ( navigator.language )
			sUserLang = navigator.language.toLowerCase() ;
		else
		{
			return 'fr' ;
		}
		
		if ( sUserLang.length >= 5 )
		{
			sUserLang = sUserLang.substr(0,5) ;
			if ( AvailableLanguages[sUserLang] ) return sUserLang ;
		}
		
		if ( sUserLang.length >= 2 )
		{
			sUserLang = sUserLang.substr(0,2) ;
			if ( AvailableLanguages[sUserLang] ) return sUserLang ;
		}
	
	return 'fr' ;
}


function TranslateElements( )
{
	var e = document.getElementsByTagName('input') ;
	var sKey, s ;
	for ( var i = 0 ; i < e.length ; i++ )
	{
		if ( sKey = e[i].getAttribute( 'strLang' ) )
		{
			if ( s = LanguageInfo[ sKey ] )
			{
				e[i].value = s ;
			}
		}
	}
}



var ActiveLanguageCode = GetActiveLanguage() ;
var ActiveLanguageName = AvailableLanguages[ ActiveLanguageCode ] ;
var LanguageInfo =  returnLanguageCodes()

// End -->
</script>
