}

var enLang =
{
Title : 'SQL Server Setup',
ServerName	: 'SQL Server',
ToolbarExpand	: 'Database',
ConnectionType 	: 'ConnectionType',
UserName 	: 'UserName',
Password 	: 'Password',
BtnSubmit 	: 'Submit',
BtnReset 	: 'Reset',
